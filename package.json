{
  "name": "ember-typetest",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for ember-typetest goes here",
  "repository": "",
  "type": "module",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc",
    "start": "vite",
    "test": "vite build --mode development && testem ci"
  },
  "imports": {
    "#app/*": "./app/*",
    "#config": "./app/config.ts"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/runtime": "^7.28.4",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.3.0",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/core": "^4.2.4",
    "@embroider/macros": "^1.19.1",
    "@embroider/router": "^3.0.4",
    "@embroider/vite": "^1.3.0",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.0.3",
    "@glint/template": "^1.6.1",
    "@glint/tsserver-plugin": "2.0.3",
    "@nullvoxpopuli/eslint-configs": "^5.3.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.11.0",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-source": "~6.7.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.37.0",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.5.0",
    "testem": "^3.16.0",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "ember-primitives": "^0.40.0",
    "ember-strict-application-resolver": "^0.1.0"
  }
}
